{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import", "jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "no-shadow": "off",
    "arrow-parens": [2, "as-needed"]
  }
}
